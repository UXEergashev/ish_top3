{
    "buildCommand": null,
    "outputDirectory": ".",
    "cleanUrls": false,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/(.*).js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/worker",
            "destination": "/worker-dashboard.html"
        },
        {
            "source": "/employer",
            "destination": "/employer-dashboard.html"
        }
    ]
}